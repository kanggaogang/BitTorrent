<configuration>
	<system.web>
		<authentication mode="None"/>
		<httpHandlers>
			<add verb="GET" path="announce" type="System.Net.BitTorrent.Tracker.AnnounceHandler,tracker" validate="false"/>
			<add verb="GET" path="scrape" type="System.Net.BitTorrent.Tracker.ScrapeHandler,tracker" validate="false"/>
			<add verb="GET" path="test" type="System.Net.BitTorrent.Tracker.TestHandler,tracker" validate="false" />
		</httpHandlers>		
	</system.web>
</configuration>